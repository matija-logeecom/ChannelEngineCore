{
    "name": "matija/channelengine",
    "description": "Channel Engine integration module for PrestaShop",
    "type": "prestashop-module",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github-logeecom:channelengine/logeecom-core.git"
        }
    ],
    "require": {
        "php": ">=8.2",
        "channelengine/integration-core": "1.1.3",
        "symfony/dependency-injection": "^4.4",
        "symfony/service-contracts": "^2.2",
        "ext-curl": "*"
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ChannelEngineCore\\": "classes/"
        }
    },
    "authors": [
        {
            "name": "Matija Stankovic",
            "email": "matija.stankovic@logeecom.com"
        }
    ]
}
